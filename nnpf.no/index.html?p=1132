			ga( 'ec:addImpression', {
				'id': '576',
				'name': '2XU FITNESS COMPRESSION TIGHTS DAME',
				'category': 'Bekledning',
				'list': 'Product List',
				'position': '4'
			} );
		


			$( '.products .post-576 a' ).on( 'click', function() {
				if ( true === $(this).hasClass( 'add_to_cart_button' ) ) {
					return;
				}

				ga( 'ec:addProduct', {
