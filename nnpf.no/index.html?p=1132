<script type='text/javascript' src='https://nnpf.no/wp-content/plugins/woocommerce/assets/js/frontend/add-to-cart-variation.min.js?ver=5.0.0' id='wc-add-to-cart-variation-js'></script>
<!-- WooCommerce JavaScript -->
<script type="text/javascript">
jQuery(function($) { 

					$( '.single_add_to_cart_button' ).on( 'click', function() {
						ga( 'ec:addProduct', {'id': '#1132','name': '2XU ACTIVE Run Tee Blå Unisex','quantity': $( 'input.qty' ).val() ? $( 'input.qty' ).val() : '1'} );
						ga( 'ec:setAction', 'add' );
						ga( 'send', 'event', 'UX', 'click', 'add to cart' );
					});
				


			ga( 'ec:addImpression', {
				'id': '1201',
				'name': 'RESTESALG T-skjorte Craft in the zone med BRY DEG si nei til doping logo dame og herremodell',
				'category': 'Bekledning',
				'list': 'Product List',
				'position': '1'
			} );
		


			$( '.products .post-1201 a' ).on( 'click', function() {
				if ( true === $(this).hasClass( 'add_to_cart_button' ) ) {
					return;
				}

				ga( 'ec:addProduct', {
					'id': '1201',
					'name': 'RESTESALG T-skjorte Craft in the zone med BRY DEG si nei til doping logo dame og herremodell',
					'category': 'Bekledning',
					'position': '1'
				});

				ga( 'ec:setAction', 'click', { list: 'Product List' });
				ga( 'send', 'event', 'UX', 'click', ' Product List' );
			});
		


			ga( 'ec:addImpression', {
				'id': '1211',
				'name': 'T-skjorte for barn Bry Deg',
				'category': 'Bekledning',
				'list': 'Product List',
				'position': '2'
			} );
		


