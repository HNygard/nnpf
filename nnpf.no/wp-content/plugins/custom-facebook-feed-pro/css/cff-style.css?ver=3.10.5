@-webkit-keyframes ctf-sk-scaleout {
    0% { -webkit-transform: scale(0) }
    100% {
        -webkit-transform: scale(1.0);
        opacity: 0;
    }
}
@keyframes ctf-sk-scaleout {
    0% {
        -webkit-transform: scale(0);
        transform: scale(0);
